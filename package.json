{
	"name": "nearest-public-place",
	"description": "A technical testing project to get nearest public place with NodeJS.",
	"version": "1.0.0",
	"author": "Tuan Le Minh <minhtuanchannhan@gmail.com>",
	"main": "src/index.js",
	"license": "MIT",
	"scripts": {
		"start": "SET NODE_ENV=production & pm2 start ./src/index.js",
		"dev": "nodemon ./src/index.js",
		"lint": "eslint ./src/**/*.js",
		"lint:fix": "yarn lint --fix",
		"test": "nyc mocha ./src/test/**/*.js --timeout=10000 --exit "
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint & yarn test",
			"pre-push": "yarn lint:fix"
		}
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:minhtuanchannhan/nearest-public-place.git"
	},
	"dependencies": {
		"@google/maps": "^1.1.0",
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-validation": "^3.0.2",
		"helmet": "^3.21.3",
		"http-status": "^1.4.2",
		"husky": "^4.2.3",
		"morgan": "^1.9.1",
		"nodemon": "^2.0.2",
		"pm2": "^4.2.3",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.20.1"
	}
}
